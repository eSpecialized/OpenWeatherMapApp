   pod 'RxCocoa', '~> 4.3.1'
   pod 'RealmSwift', '~> 3.11.1'

  target 'OpenWeatherMapAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'OpenWeatherMapAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
